#include<stdio.h>
int main()
{
    int n;
    scanf("%d",&n);
    while(n--)
    {
        int num[3];
        char b[3];
        for(int i=0;i<3;i++){
            scanf("%d%c",&num[i],&b[i]);
        }
        for(int i=0;i<2;i++)
            for(int j=i+1;j<3;j++)
                if(num[i]>num[j])
                {
                    int t=num[i];
                    num[i]=num[j];
                    num[j]=t;
                    char c=b[i];
                    b[i]=b[j];
                    b[j]=c;
                }
        int flag=6,max=num[2];
        if((num[0]==num[1])&&(num[1]==num[2])){
            flag=1;
        }
        else if((b[0]==b[1])&&(b[1]==b[2])&&(num[1]==num[0]+1)&&((num[2]==num[1]+1))){
            flag=2;
        }
        else if((num[1]==num[0]+1)&&(num[2]==num[1]+1)){
            flag=3;
        }
        else if((b[0]==b[1])&&(b[1]==b[2])){
            flag=4;
        }
        else if(num[0]==num[1]||num[1]==num[2])
        {
            flag=5;
            max=num[1];
        }
        if(max<11){
            if(flag==1){
                printf("豹子-%d\n",max);
            }
            else if(flag==2){
                printf("同花顺-%d\n",max);
            }
            else if(flag==3){
                printf("顺子-%d\n",max);
            }
            else if(flag==4){
                printf("同花-%d\n",max);
            }
            else if(flag==5){
                printf("对子-%d\n",max);
            }
            else{
                printf("花牌-%d\n",max);
            }
        }else{
            if(flag==1){
                printf("豹子-");
            }
            else if(flag==2){
                printf("同花顺-");
            }
            else if(flag==3){
                printf("顺子-");
            }
            else if(flag==4){
                printf("同花-");
            }
            else if(flag==5){
                printf("对子-");
            }
            else{
                printf("花牌-");
            }if(max==11){
                printf("J\n");
            }if(max==12){
                printf("Q\n");
            }if(max==13){
                printf("K\n");
            }if(max==14){
                printf("A\n");
            }
        }
    }
    return 0;
}
